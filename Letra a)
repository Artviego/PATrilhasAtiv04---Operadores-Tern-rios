using System;

namespace Atividade_04_Part_03
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.Write("Digite um número: ");
			int numero = int.Parse(Console.ReadLine());
			
			Console.WriteLine((numero % 2 == 0)? "O número é par" : "O número é ímpar");
			Console.ReadKey();
		}
	}
}
